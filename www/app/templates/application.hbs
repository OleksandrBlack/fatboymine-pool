<!-- Fixed navbar -->
<div class="navbar navbar-default navbar-fixed-top" role="navigation">
  <div class="container-fluid">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
      </button>
    </div>
    <div class="collapse navbar-collapse">
      <ul class="nav navbar-nav" >
	  
<li class="menu  menu--submenu menu--rel"
                            data-menu-submenu-toggle="click" data-redirect="true" aria-haspopup="true">
                            <a href="javascript:void(0);" class="menu_link">
                                <i class="menu_link-icon flaticon-network"></i>
                                <span class="menu_link-text">Pools</span>
                                <i class="m-menu__hor-arrow la la-angle-down"></i>
                                <i class="m-menu__ver-arrow la la-angle-right"></i>
                            </a>
                            <div class="m-menu__submenu m-menu__submenu--classic m-menu__submenu--left"><span
                                    class="m-menu__arrow m-menu__arrow--adjust"></span>
                                <ul class="m-menu__subnav">
                                    <li class="menu " data-redirect="true" aria-haspopup="true">
                                        <a href="http://zen.fatboymine.com/" class="menu_link" target="_blank">
                                            <span class="menu_link-text"><img src="http://fatboymine.com/img/logos/coin-zen.png" class="align-content-center" style="width: 100%; height: auto; max-height: 30px;max-width: 30px;">&nbsp;&nbsp;&nbsp;ZEN Pool (ZenCash)</span>
                                        </a>
                                    </li>
                                    <li class="menu " data-redirect="true" aria-haspopup="true">
                                        <a href="http://hush.fatboymine.com/" class="menu_link" target="_blank">
                                            <span class="menu_link-text"><img src="http://fatboymine.com/img/logos/coin-hush.png" class="align-content-center" style="width: 100%; height: auto; max-height: 30px;max-width: 30px;">&nbsp;&nbsp;&nbsp;HUSH Pool (Hush)</span>
                                        </a>
                                    </li>
                                    <li class="menu " data-redirect="true" aria-haspopup="true">
                                        <a href="http://kmd.fatboymine.com/" class="menu_link" target="_blank">
                                            <span class="menu_link-text"><img src="http://fatboymine.com/img/logos/coin-kmd.png" class="align-content-center" style="width: 100%; height: auto; max-height: 30px;max-width: 30px;">&nbsp;&nbsp;&nbsp;KMD Pool (Komodo)</span>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </li>
	  
        {{#active-li currentWhen='help'}}
          {{#link-to (t 'menu.i18n.help')}}
            <i class="fa fa-rocket"></i> {{t 'menu.help'}}
          {{/link-to}}
        {{/active-li}}
        {{#active-li currentWhen='blocks'}}
          {{#link-to 'blocks'}}
            <i class="fa fa-cubes"></i> {{t 'menu.pool_blocks'}}
            {{#if immatureTotal}}
              <span class="badge alert-success">{{immatureTotal}}</span>
            {{/if}}
          {{/link-to}}
        {{/active-li}}
        {{#active-li currentWhen='payments'}}
          {{#link-to 'payments'}}
            <i class="fa fa-paper-plane-o"></i> {{t 'menu.payments'}}
          {{/link-to}}
        {{/active-li}}
        {{#active-li currentWhen='miners'}}
          {{#link-to 'miners'}}
            <i class="fa fa-users"></i> {{t 'menu.miners'}}
          {{#if model.minersTotal}}
              <span class="badge alert-success">{{model.minersTotal}}</span>
          {{/if}}
          {{/link-to}}
        {{/active-li}}
        {{#active-li currentWhen='about'}}
          {{#link-to (t 'menu.i18n.about')}}
            <i class="fa fa-comments"></i> {{t 'menu.about'}} <span class="badge alert-success">&#x2713;</span>
          {{/link-to}}
        {{/active-li}}
		{{#active-li}}
			<a class="fa fa-money" title="{{config.COIN}} Current Price">
				{{#if isPercentAboveZero}}
					${{format-number model.exchangedata.price_usd maximumFractionDigits='2'}} <span class="badge alert-success"><i class="fa fa-caret-up"></i> (+{{model.exchangedata.percent_change_24h}}%)</span>
				{{else}}
					${{format-number model.exchangedata.price_usd maximumFractionDigits='2'}} <span class="badge alert-danger"><i class="fa fa-caret-down"></i> ({{model.exchangedata.percent_change_24h}}%)</span>
				{{/if}}
			</a>
        {{/active-li}}
		{{#active-li}}
				<a class="nav-item nav-link dropdown-toggle" data-toggle="dropdown" id="selectedLanguage">
					{{selectedLanguage}}
					<b class="caret"></b>
				</a>
				<ul class="dropdown-menu dropdown-menu-right">
					{{#each languages as |lang|}}
						<li>
							<a href="#" {{action 'selectLanguage' lang.value on='click'}}>{{lang.name}}</a>
						</li>
					{{/each}}
				</ul>
        {{/active-li}}
	</ul>
    </div>
  </div>
</div>

{{outlet}}